+<?php
+
+namespace App\Http\Controllers;
+
+use Illuminate\Foundation\Bus\DispatchesJobs;
+use Illuminate\Routing\Controller as BaseController;
+use Illuminate\Foundation\Validation\ValidatesRequests;
+use Illuminate\Foundation\Auth\Access\AuthorizesRequests;
+use MongoClient;
+
+class Controller extends BaseController
+{
+	use AuthorizesRequests, DispatchesJobs, ValidatesRequests;
+
+	public function insert($name = null, $price = null) {
+		$db = new MongoClient('mongodb://localhost');
+		$tasks = $db->selectCollection("scotchbox", "tasks");
+		if (strlen($name) > 1) {
+			$name = (string)$name;		
+			$price = (int)$price;
+			$tasks->update(['name' => $name], ['$set' => ['name' => $name, 'price' => $price]], ['upsert' => true]);
+		}
+		$results = $this->showAll($tasks);
+		return view('paytask')->with('tasks', $results);// $data);
+	}
+
+	public function showAll($db) {
+
+		$tasks = $db;
+		$results = [];
+		$results[] = "<br/>";
+		$sortedTasks = $tasks->find()->sort(['price' => 1]);
+		return $this->throttle($sortedTasks);
+	}
+
+	public function throttle($results, $all = false) {
+		$paycheck = 1700;
+		$total = 0;
+		$tasks = [];
+		foreach ($results as $result) {
+			if ($all || $result['price'] + $total <= $paycheck) {
+				$total += $result['price'];
+				$tasks[] = $result;
+			}
+		}
+		return $tasks;
+	}
+}
